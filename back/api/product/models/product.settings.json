{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "products",
  "info": {
    "name": "Product"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "image": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "price": {
      "type": "float"
    },
    "shop": {
      "model": "shop",
      "via": "products"
    }
  }
}
